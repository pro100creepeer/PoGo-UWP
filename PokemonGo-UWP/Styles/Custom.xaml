    <Style x:Key="TextBox" TargetType="TextBox">
        <Setter Property="Foreground">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                    <GradientStop Color="#a8e090" Offset="0"/>
                    <GradientStop Color="#35908f" Offset="0.8"/>
                </LinearGradientBrush>
            </Setter.Value>
